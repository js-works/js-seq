{
  "name": "js-seq",
  "version": "0.0.1",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/js-works/js-seq",
  "main": "dist/js-seq.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/js-works/js-seq.git"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "browserify": "^14.4.0",
    "chai": "^4.1.2",
    "cross-conf-env": "^1.1.2",
    "mocha": "^5.1.1",
    "ngzip": "^1.1.3",
    "shx": "^0.2.2",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typedoc": "^0.11.1",
    "uglify-js": "^3.3.22"
  },
  "scripts": {
    "clean": "node_modules/.bin/shx rm -rf ./build ./dist",
    "compile": "npm run clean && tsc",
    "bundle": "npm run compile && \"node_modules/.bin/shx\" mkdir -p dist && \"node_modules/.bin/browserify\" build/main/js-seq.js -s jsSeq -o dist/js-seq.js",
    "minimize": "npm run bundle && \"node_modules/.bin/uglifyjs\" dist/js-seq.js -o dist/js-seq.min.js --compress --mangle",
    "compress": "npm run minimize && \"node_modules/.bin/ngzip\" -9 dist/js-seq.min.js > dist/js-seq.min.js.gz",
    "test": "node_modules/.bin/mocha -r ts-node/register src/test/**/*.ts",
    "test-watch": "node_modules/.bin/mocha -w -r ts-node/register src/test/**/*.ts",
    "docs": "node_modules/.bin/shx rm -rf ./dist/docs && \"node_modules/.bin/cross-conf-env\" typedoc --options typedoc.json src/main/api/ -name \"js-seq vnpm_package_version\"",
    "all": "npm run compress && npm run test && npm run docs && \"node_modules/.bin/shx\" echo Finished successfully."
  },
  "dependencies": {
    "js-spec": "^0.1.20",
    "react": "^16.3.2"
  }
}
